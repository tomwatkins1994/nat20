{
    "author": "Tom Watkins",
    "name": "nat20",
    "version": "1.0.9",
    "repository": {
        "type": "git",
        "url": "https://github.com/tomwatkins1994/nat20.git"
    },
    "description": "A package for simulating dice rolls",
    "main": "dist/index.js",
    "files": [
        "package.json",
        "readme.md",
        "dist/*"
    ],
    "scripts": {
        "test": "vitest",
        "test:ui": "vitest --ui",
        "build": "tsc"
    },
    "keywords": [
        "Dice",
        "Roll",
        "RPG"
    ],
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/tomwatkins1994/nat20/issues"
    },
    "homepage": "https://github.com/tomwatkins1994/nat20#readme",
    "dependencies": {
        "mathjs": "^7.2.0"
    },
    "devDependencies": {
        "@types/mathjs": "^6.0.5",
        "@types/node": "^14.6.4",
        "@vitest/ui": "^2.1.4",
        "typescript": "^3.9.7",
        "vitest": "^2.1.4"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm start",
        "ext": "ts"
    }
}